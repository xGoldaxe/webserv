server
{
	server_name = test ;
	listen = 3000 ;
	error_page 404 403 = error.html ;
	body_max_size = 2048 ;

	location /
	{
		root ./ ;
		index = index.html index.htm ;
		error_page 404 = 404.html ;
		methods = POST GET DELETE ;
		rewrite  /sub  moved.html ;
		autoindex on;
	}
	location /php 
	{ 
		root ./php ;
		index = index.php ;
		enable_cgi $CGI_PATH ; 
		cgi_extension php ;
	}
}